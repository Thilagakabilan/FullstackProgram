class Circle{
    constructor(radius,color)
    {
        this.radius=radius;
        this.color=color;
    }
    circle()
    {
    console.log("Area and circumference of the circle")
    }
    circle(radius)
    {
        console.log(`Radius of thr circle : ${this.radius}`);
    }
    circle(radius,color)
    {
        console.log(`Radius and Color of thr circle : ${this.radius},${this.color}`);
    }
    getRadius()
    {
        return this.radius;
    }
    setRadius(radius)
    {
        this.radius=radius;
    }
    getColor()
    {
        return this.color;
    }
    setColor(color)
    {
        this.color=color;
    }
    getArea()
    {
        let area,r=this.radius;
        area=3.14*r*r;
        console.log(area);
    }
    getCircumference()
    {
        let r=this.radius,circum;
        circum=2*3.14*r;
        console.log(circum);
    }
}
let objcircle= new Circle(1,"red");
objcircle.getArea();
objcircle.getCircumference();
